<script>

	const ham = document.getElementById('ham')
	ham?.addEventListener('click', () => {
		const elem1 = document.getElementById('ham_elem1')
		const elem2 = document.getElementById('ham_elem2')
		const sp_nav = document.getElementById('sp_nav')
		const sp_header = document.getElementById('sp_header')

		if (sp_nav?.classList.contains('dis-none')) {
			sp_nav?.classList.remove('dis-none')
			sp_header?.classList.add('height_100dvh')
			sp_header?.classList.remove('height_0')
			elem1?.classList.add('elem1-lotate-in')
			elem2?.classList.add('elem2-lotate-in')
			elem1?.classList.remove('elem1-lotate-out')
			elem2?.classList.remove('elem2-lotate-out')

		} else {
			sp_nav?.classList.add('dis-none')
			sp_header?.classList.remove('height_100dvh')
			sp_header?.classList.add('height_0')
			elem1?.classList.add('elem1-lotate-out')
			elem2?.classList.add('elem2-lotate-out')
			elem1?.classList.remove('elem1-lotate-in')
			elem2?.classList.remove('elem2-lotate-in')
		}
	})

	document.addEventListener('DOMContentLoaded', () => {
  const aside_min_btn = document.getElementById('aside_min_btn');
  const aside = document.getElementById('aside');
  const aside_min_inner = document.getElementById('aside_inner');
  const main = document.getElementById('main');

  // ページ読み込み時に状態を復元
  const isAsideMinimized = localStorage.getItem('aside_minimized') === 'true';
  if (isAsideMinimized) {
    minimizeAside();
  }

  aside_min_btn?.addEventListener('click', () => {
    const currentlyMinimized = aside?.classList.contains('aside_min');

    if (currentlyMinimized) {
      maximizeAside();
      localStorage.setItem('aside_minimized', 'false');
    } else {
      minimizeAside();
      localStorage.setItem('aside_minimized', 'true');
    }
  });

  function minimizeAside() {
    document.querySelectorAll('#min_item').forEach((min_item) => {
      min_item.classList.add('dis-none');
    });
    aside?.classList.add('aside_min');
    aside_min_inner?.classList.add('aside_min_inner');
    main?.classList.add('main_min');
  }

  function maximizeAside() {
    document.querySelectorAll('#min_item').forEach((min_item) => {
      min_item.classList.remove('dis-none');
    });
    aside?.classList.remove('aside_min');
    aside_min_inner?.classList.remove('aside_min_inner');
    main?.classList.remove('main_min');
  }
});


	const sidebar = document.getElementById('sidebar')
	const newssidebar = document.getElementById('newssidebar')
	const sp_modal = document.getElementById('sp_modal')
	const sp_news_modal = document.getElementById('sp_news_modal')
	const sp_classmodal_close = document.getElementById('sp_classmodal_close')
	const sp_classmodal_back = document.getElementById('sp_classmodal_back')

	document.querySelectorAll('.sidebar_item').forEach((sidebar_item) => {
		const close_btn = document.getElementById('handle_sidebar_close')
		sidebar_item.addEventListener('click', () => {
			const sidebar_title = document.getElementById('sidebar_title')
			const sidebar_text = sidebar_item.querySelector('.class_name')?.textContent?.trim()
			console.log(sidebar_title)
			console.log(sidebar_text)

			if (window.innerWidth <= 780) {
      // 画面サイズが780px以下の場合
        if (sp_modal?.classList.contains('dis-none')) {
          sp_modal?.classList.remove('dis-none');
          sp_modal?.classList.add('dis-flex');
        } 

      } else {
        // 画面サイズが780pxより大きい場合
        if (newssidebar?.classList.contains('dis-block')) {
          newssidebar?.classList.remove('dis-block');
          newssidebar?.classList.add('dis-none');
        }

        if (sidebar?.classList.contains('dis-none')) {
          sidebar?.classList.add('dis-block');
          sidebar?.classList.remove('dis-none');
        }

        if (sidebar_title && sidebar_text) {
          sidebar_title.textContent = sidebar_text;
        }
      }
		})

		sp_classmodal_close?.addEventListener('click', () => {
			sp_modal?.classList.add('dis-none');
			sp_modal?.classList.remove('dis-flex');
		})

		sp_classmodal_back?.addEventListener('click', () => {
			sp_modal?.classList.add('dis-none');
			sp_modal?.classList.remove('dis-flex');
		})

		close_btn?.addEventListener('click', () =>{
			sidebar?.classList.remove('dis-block')
			sidebar?.classList.add('dis-none')
		})
	})

