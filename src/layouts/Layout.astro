---
// Propsのインターフェース
interface Props {
  title: string;
	subtitle?: string
  headerTitle: string;
  URL?: string;
  href?: string;
  ogptitle?: string;
  type?: string;
  description?: string;
  sns_img?: string;
  paths?: { label: string; url: string }[];
}

// Astro.propsからプロパティを取得
const { title, URL, ogptitle, type, description, sns_img, paths, subtitle } = Astro.props;

// 現在のパスを取得
const currentPath = Astro.url.pathname;

const isGradesPage = currentPath === "/grades/";
