		news_modal_back?.addEventListener('click', () => {
			sp_news_modal?.classList.add('dis-none');
			sp_news_modal?.classList.remove('dis-flex');
		})

		close_btn?.addEventListener('click', () =>{
			newssidebar?.classList.remove('dis-block')
			newssidebar?.classList.add('dis-none')
		})
	})
});


